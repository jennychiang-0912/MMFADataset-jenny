      "source": [
        "from tensorflow.keras.preprocessing.text import Tokenizer\n",
        "from tensorflow.keras.preprocessing.sequence import pad_sequences\n",
        "\n",
        "# 1. Initialize Tokenizer\n",
        "# num_words: the maximum number of words to keep, based on word frequency.\n",
        "# oov_token: a token to represent out-of-vocabulary words.\n",
        "tokenizer = Tokenizer(num_words=None, oov_token=\"<unk>\") # Using None to keep all words initially, or set a specific number if needed\n",
        "\n",
        "# 2. Fit Tokenizer on training data to build the vocabulary\n",
        "tokenizer.fit_on_texts(X_train_segmented)\n",
        "\n",
        "# Get vocabulary size (+1 for padding/oov_token if used)\n",
        "vocab_size = len(tokenizer.word_index) + 1\n",
        "print(f\"Vocabulary size: {vocab_size}\")\n",
        "\n",
        "# 3. Convert text to sequences\n",
        "X_train_sequences = tokenizer.texts_to_sequences(X_train_segmented)\n",
        "X_test_sequences = tokenizer.texts_to_sequences(X_test_segmented)\n",
        "\n",
        "# 4. Determine an appropriate maximum sequence length\n",
        "# A common approach is to look at the distribution of sequence lengths\n",
        "# For simplicity, we'll choose a fixed value or analyze the current data\n",
        "train_lengths = [len(x) for x in X_train_sequences]\n",
        "test_lengths = [len(x) for x in X_test_sequences]\n",
        "\n",
        "# Let's find a reasonable max length (e.g., 95th percentile or a fixed value like 256)\n",
        "MAX_SEQUENCE_LENGTH = 256 # A common choice\n",
        "# Alternatively, calculate based on data:\n",
        "# MAX_SEQUENCE_LENGTH = int(np.percentile(train_lengths + test_lengths, 95))\n",
        "# print(f\"Calculated Max Sequence Length (95th percentile): {MAX_SEQUENCE_LENGTH}\")\n",
        "\n",
        "# 5. Pad sequences to a uniform length\n",
        "X_train_padded = pad_sequences(X_train_sequences, maxlen=MAX_SEQUENCE_LENGTH, padding='post', truncating='post')\n",
        "X_test_padded = pad_sequences(X_test_sequences, maxlen=MAX_SEQUENCE_LENGTH, padding='post', truncating='post')\n",
        "\n",
        "# 6. Print the shape of the padded data\n",
        "print(f\"Shape of X_train_padded: {X_train_padded.shape}\")\n",
        "print(f\"Shape of X_test_padded: {X_test_padded.shape}\")\n",
