      "source": [
        "from sklearn.metrics import confusion_matrix, classification_report\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "# 1. Evaluate the model on the test set\n",
        "loss_cklp_cnn, accuracy_cklp_cnn = model_cklp_cnn.evaluate(X_test_padded, y_test_numerical)\n",
        "\n",
        "# 2. Print the test loss and accuracy\n",
        "print(f\"\\nTest Loss (Deeper CNN): {loss_cklp_cnn:.4f}\")\n",
        "print(f\"Test Accuracy (Deeper CNN): {accuracy_cklp_cnn:.4f}\")\n",
        "\n",
        "# 3. Make predictions on the test set and convert probabilities to binary labels\n",
        "y_pred_prob_cklp_cnn = model_cklp_cnn.predict(X_test_padded).ravel() # Probabilities 0~1\n",
        "y_pred_cklp_cnn = (y_pred_prob_cklp_cnn >= 0.5).astype(int)              # Binary classification results 0/1\n",
        "\n",
        "# 4. Calculate the confusion matrix\n",
        "cm_cklp_cnn = confusion_matrix(y_test_numerical, y_pred_cklp_cnn)\n",
        "\n",
        "# 5. Plot the confusion matrix\n",
        "plt.figure(figsize=(8, 6))\n",
        "sns.heatmap(cm_cklp_cnn, annot=True, fmt='d', cmap='Blues',\n",
        "            xticklabels=['Predicted Negative (0)', 'Predicted Positive (1)'],\n",
        "            yticklabels=['True Negative (0)', 'True Positive (1)'])\n",
        "plt.xlabel('Predicted label')\n",
        "plt.ylabel('True label')\n",
        "plt.title('Confusion Matrix - Deeper CNN Model Test Set')\n",
        "plt.show()\n",
        "\n",
        "# 6. Generate and print the classification report\n",
        "report_cklp_cnn = classification_report(y_test_numerical, y_pred_cklp_cnn)\n",
        "print(\"\\nClassification Report (Deeper CNN):\")\n",
        "print(report_cklp_cnn)\n",
        "\n",
        "# 7. Display some example predictions to check performance\n",
        "results_cklp = pd.DataFrame({\n",
        "    \"text\": X_test,\n",
        "    \"true\": y_test_numerical,\n",
        "    \"pred\": y_pred_cklp_cnn\n",
        "})\n",
        "\n",
        "tn_cklp = results_cklp[(results_cklp.true == 0) & (results_cklp.pred == 0)]\n",
        "tp_cklp = results_cklp[(results_cklp.true == 1) & (results_cklp.pred == 1)]\n",
        "fn_cklp = results_cklp[(results_cklp.true == 1) & (results_cklp.pred == 0)]\n",
        "fp_cklp = results_cklp[(results_cklp.true == 0) & (results_cklp.pred == 1)]\n",
        "\n",
        "examples_cklp = []\n",
        "if len(tn_cklp) > 0: examples_cklp.append((\"True Negative\", tn_cklp.sample(min(len(tn_cklp), 1))))\n",
        "if len(tp_cklp) > 0: examples_cklp.append((\"True Positive\", tp_cklp.sample(min(len(tp_cklp), 1))))\n",
        "if len(fn_cklp) > 0: examples_cklp.append((\"False Negative\", fn_cklp.sample(min(len(fn_cklp), 1))))\n",
        "if len(fp_cklp) > 0: examples_cklp.append((\"False Positive\", fp_cklp.sample(min(len(fp_cklp), 1))))\n",
        "\n",
        "# If there are fewer than 4 distinct types of examples, fill with random ones\n",
        "if len(examples_cklp) < 4:\n",
        "    remaining_samples = results_cklp[~results_cklp.index.isin(pd.concat([x[1] for x in examples_cklp]).index)]\n",
        "    more_samples = remaining_samples.sample(min(len(remaining_samples), 4 - len(examples_cklp)))\n",
        "    for _, row in more_samples.iterrows():\n",
        "        examples_cklp.append((\"Random\", row.to_frame().T))\n",
        "\n",
        "for label, row in examples_cklp:\n",
        "    print(f\"\\n[{label}]\")\n",
        "    print(\"Text:\", row[\"text\"].values[0])\n",
        "    print(\"True Label:\", row[\"true\"].values[0])\n",
