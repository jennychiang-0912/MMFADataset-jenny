      "source": [
        "from sklearn.metrics import confusion_matrix, classification_report\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "# 1. Evaluate the model on the test set\n",
        "loss_cnn, accuracy_cnn = model_cnn.evaluate(X_test_padded, y_test_numerical)\n",
        "\n",
        "# 2. Print the test loss and accuracy\n",
        "print(f\"\\nTest Loss (CNN): {loss_cnn:.4f}\")\n",
        "print(f\"Test Accuracy (CNN): {accuracy_cnn:.4f}\")\n",
        "\n",
        "# 3. Make predictions on the test set and convert probabilities to binary labels\n",
        "y_pred_prob_cnn = model_cnn.predict(X_test_padded).ravel() # Probabilities 0~1\n",
        "y_pred_cnn = (y_pred_prob_cnn >= 0.5).astype(int)              # Binary classification results 0/1\n",
        "\n",
        "# 4. Calculate the confusion matrix\n",
        "cm_cnn = confusion_matrix(y_test_numerical, y_pred_cnn)\n",
        "\n",
        "# 5. Plot the confusion matrix\n",
        "plt.figure(figsize=(8, 6))\n",
        "sns.heatmap(cm_cnn, annot=True, fmt='d', cmap='Blues',\n",
        "            xticklabels=['Predicted Negative (0)', 'Predicted Positive (1)'],\n",
        "            yticklabels=['True Negative (0)', 'True Positive (1)'])\n",
        "plt.xlabel('Predicted label')\n",
        "plt.ylabel('True label')\n",
        "plt.title('Confusion Matrix - CNN Model Test Set')\n",
        "plt.show()\n",
        "\n",
        "# 6. Generate and print the classification report\n",
        "report_cnn = classification_report(y_test_numerical, y_pred_cnn)\n",
        "print(\"\\nClassification Report (CNN):\")\n",
        "print(report_cnn)\n"
      ],
