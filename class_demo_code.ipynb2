      "source": [
        "from tensorflow.keras.models import Sequential\n",
        "from tensorflow.keras.layers import Embedding, Conv1D, GlobalMaxPooling1D, Dense, Dropout\n",
        "from tensorflow.keras.optimizers import Adam\n",
        "\n",
        "# 1. Build the 1D CNN model\n",
        "model_cnn = Sequential([\n",
        "    embedding_layer,  # Use the embedding layer created previously\n",
        "    Conv1D(filters=128, kernel_size=5, activation='relu'), # Convolutional layer\n",
        "    GlobalMaxPooling1D(), # Global Max Pooling layer to reduce dimensions\n",
        "    Dense(64, activation='relu'), # Dense layer\n",
        "    Dropout(0.5), # Dropout for regularization\n",
        "    Dense(1, activation='sigmoid') # Output layer for binary classification\n",
        "])\n",
        "\n",
        "# 2. Compile the model\n",
        "model_cnn.compile(optimizer=Adam(learning_rate=0.001),\n",
        "                  loss='binary_crossentropy',\n",
        "                  metrics=['accuracy'])\n",
        "\n",
        "# 3. Print the model summary\n",
        "model_cnn.summary()\n",
        "\n",
        "print(\"1D CNN model built and compiled successfully.\")"
